// Imports example.
import PrintLn "crt.lib" "PRINTLN"
import InputLn "crt.lib" "INPUTLN"

// Program entry point (because we don't write "uses <bf>").
proc __EntryPoint():
  var a = 0, b = 1, tmp = 0
  PrintLn(0)
  PrintLn(1)
  for(var i = 0; i < 40; inc i):
    tmp = b
    b = b + a
    a = tmp
    PrintLn(b)
  end
  InputLn()
end
